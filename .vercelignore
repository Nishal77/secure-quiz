.next/
out/
dist/
build/

# Environment files (use Vercel dashboard instead)
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# OS files
.DS_Store
Thumbs.db

# Supabase
.supabase/

# Lock files (optional - Vercel will use them)
# pnpm-lock.yaml
# package-lock.json
# yarn.lock

